<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:cours_mvvm1.MVVM.ViewModel"
                    xmlns:view="clr-namespace:cours_mvvm1.MVVM.View"
                    >
    <DataTemplate DataType="{x:Type vm:HomeMV}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomersMV}">
        <view:Customers/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsMV}">
        <view:Products/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrdersMV}">
        <view:Orders/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionsMV}">
        <view:Transactions/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShipmentsMV}">
        <view:Shipments/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsMV}">
        <view:Settings/>
    </DataTemplate>
    
</ResourceDictionary>